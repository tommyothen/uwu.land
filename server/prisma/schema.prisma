generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "mongodb"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Log {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  level   Level
  message String
  meta    Json
  date    DateTime @default(now())
}

enum Level {
  Info
  Warn
  Error
}

model Redirect {
  slug   String @id @map("_id")
  url    String
  clicks Int    @default(0)
}
